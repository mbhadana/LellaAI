{
  "name": "leelav1",
  "version": "1.0.0",
  "description": "Leela V1 - AI Assistant",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:protect": "node build_protected.js",
    "dist": "npm run build:protect && electron-builder"
  },
  "build": {
    "appId": "com.leelav1.assistant",
    "productName": "Leela V1",
    "icon": "assets/icon.ico",
    "directories": {
      "output": "release",
      "app": "dist_protected"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "Leela V1"
    }
  },
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^26.8.1",
    "electron-packager": "^17.1.2",
    "javascript-obfuscator": "^5.3.0",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.5"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "bytenode": "^1.5.7",
    "ffmpeg-static": "^5.3.0",
    "form-data": "^4.0.5",
    "sarvamai": "^1.0.0"
  },
  "author": "Antigravity",
  "license": "MIT"
}